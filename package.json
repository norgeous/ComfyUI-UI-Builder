{
  "name": "ui-builder",
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prelint": "prettier --log-level silent --write \"**/*.{js,cjs,jsx,md,json,html,css,yml}\"",
    "lint": "eslint . --ext .js,.jsx,.cjs --fix",
    "sb": "npm run sb -w packages/storybook",
    "sb:build": "npm run build -w packages/storybook",
    "sb:unittest": "npm run unittest -w packages/storybook",
    "sb:unittest:ci": "npm run unittest:ci -w packages/storybook",
    "sb:storycap": "npm run storycap -w packages/storybook",
    "outdated": "npm outdated"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.18",
    "colorjs.io": "^0.5.2",
    "microphone-stream": "^6.0.1",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "screenfull": "^6.0.2",
    "styled-components": "^6.1.11",
    "vite-compatible-readable-stream": "^3.6.1",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vosk-browser": "^0.0.8"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-storybook": "^0.8.0",
    "vite": "^5.3.2"
  },
  "peerDependencies": {
    "@storybook/test": "^8.1.9"
  }
}
