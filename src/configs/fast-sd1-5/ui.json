{
  "name": "Fast SD1.5",
  "version": "v0.0.0",
  "emoji": "â©",
  "description": "Input prompt with voice, eventually this will be a part of InputTextarea / all prompts by default",
  "theme": {
    "radius": "0px",
    "scheme": "cyan darkgoldenrod"
  },
  "tags": ["SD1.5", "S2I"],
  "formConfig": [
    {
      "group": "Prompt",
      "id": "positivePrompt",
      "label": "Positive Prompt",
      "type": "speech",
      "defaultValue": "",
      "colSpan": 2
    },
    {
      "group": "Prompt",
      "id": "negativePrompt",
      "label": "Negative Prompt",
      "type": "speech",
      "defaultValue": "",
      "colSpan": 2
    },
    {
      "group": "Generation Params",
      "id": "ckpt",
      "label": "Checkpoint",
      "info": "Choose a SDXL checkpoint",
      "type": "selectckpt",
      "defaultValue": "",
      "colSpan": 2
    },
    {
      "group": "Generation Params",
      "id": "size",
      "label": "Size",
      "type": "select",
      "defaultValueIndex": 0,
      "options": [
        {
          "label": "1:1 Square",
          "value": { "width": 512, "height": 512 }
        },
        {
          "label": "16:9 Landscape",
          "value": { "width": 768, "height": 512 }
        },
        {
          "label": "16:9 Portrait",
          "value": { "width": 512, "height": 768 }
        }
      ]
    },
    {
      "group": "Generation Params",
      "id": "seed",
      "label": "Seed",
      "type": "seed",
      "defaultValue": { "seed": 1, "random": false },
      "min": 0
    }
  ],
  "adapterConfig": [
    {
      "destination": "CheckpointLoaderSimple > ckpt_name",
      "actions": "get:ckpt"
    },
    {
      "destination": "Positive Prompt > text",
      "actions": "get:positivePrompt"
    },
    {
      "destination": "EmptyLatentImage > width",
      "actions": "get:width"
    },
    {
      "destination": "EmptyLatentImage > height",
      "actions": "get:height"
    },
    {
      "destination": "KSampler > seed",
      "actions": "get:seed"
    }
  ]
}
