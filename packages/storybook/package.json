{
  "name": "@ui-builder/storybook",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "sb": "storybook dev -p 6006",
    "build": "storybook build --quiet",
    "serve": "serve -l 6006 -L storybook-static",
    "unittest": "wait-on tcp:6006 && test-storybook --stories-json --maxWorkers=1 --no-cache",
    "unittest:ci": "concurrently -k -s command-TEST -n SERVE,TEST -c magenta,blue npm:serve \"npm:unittest -- --ci\"",
    "storycap": "storycap --serverCmd \"npm run build && npm run serve\" --outDir ../../_storycaps_ http://localhost:6006",
    "storycap:ci": "storycap --serverCmd \"npm run serve\" --outDir ../../_storycaps_ http://localhost:6006",
    "regression": "reg-suit compare",
    "serve:visdiff": "serve -l 3000 ./visdiff-report/"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "@storybook/addon-a11y": "^8.1.11",
    "@storybook/addon-actions": "^8.1.11",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-themes": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/react-vite": "^8.1.11",
    "@storybook/react-webpack5": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@storybook/test-runner": "^0.19.0",
    "axe-playwright": "^2.0.1",
    "concurrently": "^8.0.1",
    "msw": "^2.3.1",
    "msw-storybook-addon": "^2.0.2",
    "puppeteer": "^22.12.1",
    "reg-suit": "^0.14.4",
    "serve": "^14.2.0",
    "storybook": "^8.1.11",
    "storycap": "^5.0.0",
    "wait-on": "^7.0.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.18.0",
    "@swc/core": "^1.6.3",
    "@swc/core-darwin-arm64": "^1.6.3",
    "@swc/core-darwin-x64": "^1.6.3",
    "@swc/core-linux-arm-gnueabihf": "^1.6.3",
    "@swc/core-linux-arm64-gnu": "^1.6.3",
    "@swc/core-linux-arm64-musl": "^1.6.3",
    "@swc/core-linux-x64-gnu": "^1.6.3",
    "@swc/core-linux-x64-musl": "^1.6.3",
    "@swc/core-win32-arm64-msvc": "^1.6.3",
    "@swc/core-win32-ia32-msvc": "^1.6.3",
    "@swc/core-win32-x64-msvc": "^1.6.3"
  }
}
